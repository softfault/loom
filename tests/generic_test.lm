[Box<T>]
# 这里初始化为 nil。
# 注意：在 Loom 中，如果 T 是 int，Runtime 初始值为 Nil 可能会在做数学运算时报错
# 但这里我们只是存取，所以没问题。
val: T 

set = (new_val: T)
    self.val = new_val

get = () T
    return self.val

[Printer<U>]
print_it = (item: U)
    print("Printer says: " + item)

[Main]
main = () int
    # === 测试 1: Int 泛型 ===
    # 语法: Box<int>() 
    # Parser 会解析为 CallExpr，其中 generic_args = [int]
    # Interpreter 的 eval_call 会忽略这个 [int]，直接实例化 Box
    i_box = Box<int>()
    
    i_box.set(100)
    # i_box.set("error") # 如果取消注释，Analyzer 应该报错
    
    print("Int Box: " + i_box.get())

    # === 测试 2: Str 泛型 ===
    s_box = Box<str>()
    s_box.set("Hello Generics")
    print("Str Box: " + s_box.get())

    # === 测试 3: 另一个泛型类 ===
    p = Printer<str>()
    p.print_it("World")

    return 0