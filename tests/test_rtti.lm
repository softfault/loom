[Animal]
type_name: str = "Generic Animal"

[Dog : Animal]
breed: str = "Shiba"
bark = () => print("Woof! I am a " + self.breed)

[Cat : Animal]
meow = () => print("Meow!")

[main()]
    # 1. 创建一个 Dog，但是用 Animal 盒子装着 (Upcast)
    my_pet: Animal = Dog() as Animal
    print("Created a pet: " + my_pet.type_name)

    # 2. 安全的向下转型 (Downcast: Animal -> Dog)
    # 运行时会检查 my_pet 到底是不是 Dog
    d = my_pet as Dog
    d.bark() # 应该成功输出

    # 3. 失败的向下转型 (Downcast: Animal -> Cat)
    # my_pet 本质是 Dog，不能转成 Cat
    print("Trying to turn Dog into Cat...")
    c = my_pet as Cat 
    c.meow() # 这行不应该执行