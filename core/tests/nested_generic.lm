[Container<T>]
item: T 
set = (v: T) 
    self.item = v
get = () T 
    return self.item

[Main]
main = () int
    # 1. 实例化内层
    inner = Container<int>()
    inner.set(42)

    # 2. 实例化外层：Container 里面装的是 Container<int>
    outer = Container<Container<int>>()
    
    outer.set(inner)

    # 3. 链式调用
    # outer.get() 返回 inner
    # inner.get() 返回 42
    res = outer.get().get()
    
    print("Nested Result: " + res)
    return 0