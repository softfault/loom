[Animal]
speak = () => print("...")

[Dog : Animal]
speak = () => print("Woof")

[Cat : Animal]
speak = () => print("Meow")

[Box<T>]
val: T 

set = (new_val: T)
    self.val = new_val

get = () T
    return self.val

[Trainer]
# 参数接收 Animal，但可以传 Dog 或 Cat
train = (a: Animal) 
    print("Training an animal...")
    a.speak() # 动态分派

[Main]
main = () int
    trainer = Trainer()
    d = Dog()
    c = Cat()

    trainer.train(d) # 应该打印 Woof
    trainer.train(c) # 应该打印 Meow
    
    box_dog = Box<Dog>()
    box_animal: Box<Animal> = box_dog

    return 0